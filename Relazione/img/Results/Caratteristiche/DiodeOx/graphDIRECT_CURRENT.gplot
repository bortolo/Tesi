load '../header'

#set format x "%.1t {/Symbol \327} 10^{%T}"
set grid
set xrange [0.0:1.5]
#set xrange [0.8:1.5]
#set logscale x 
set xlabel 'V [V]' font "Helvetica bold,20"

set yrange [1e-19:1e-3]
#set yrange [1e-6:1e-3]
set format y "10^{%T}"
set ytics nomirror
set logscale y;
set ylabel 'I [A]' font "Helvetica bold,20" 
#set ylabel 'Electrostatic Potential [V]' font "Helvetica bold,20" 

set lmargin 10


set key bottom right
set key font "Helvetica,12"
set key box
set key reverse
set key Left


#set title 'Diode Oxide test 1817 - Current at P-contact' font "Helvetica bold,20"
#plot './DiodeOxideDirectPolarizationCaratteristicaVI.txt' u 5:(abs($6)) ti 'In FEMOS' w l ls 11
