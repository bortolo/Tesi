set style arrow 16 filled size screen 0.01,20,35 ls 16
set style arrow 17 filled size screen 0.01,20,35 ls 17
set style arrow 18 filled size screen 0.01,20,35 ls 18



# arrow style:  continuos line open arrow

set style arrow 20 head empty size screen 0.03,15,135 ls 10
set style arrow 21 head empty size screen 0.03,15,135 ls 11
set style arrow 22 head empty size screen 0.03,15,135 ls 12
set style arrow 23 head empty size screen 0.03,15,135 ls 13
set style arrow 24 head empty size screen 0.03,15,135 ls 14
set style arrow 25 head empty size screen 0.03,15,135 ls 15
set style arrow 26 head empty size screen 0.03,15,135 ls 16
set style arrow 27 head empty size screen 0.03,15,135 ls 17
set style arrow 28 head empty size screen 0.03,15,135 ls 18


