load '../header'

#set format x "%.1t {/Symbol \327} 10^{%T}"
set grid
set xrange [0.0:0.1]
#set logscale x 
set xlabel 'Z axis [micron]' font "Helvetica bold,20"

set yrange [0.3:1.2]
#set format y "10^{%T}"
set ytics nomirror
