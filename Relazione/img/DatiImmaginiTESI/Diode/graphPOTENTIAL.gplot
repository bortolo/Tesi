set ylabel 'Potential [V]' font "Helvetica bold,20" 
#set ylabel 'Electrostatic Potential [V]' font "Helvetica bold,20" 

set lmargin 10

# Opzioni legenda
set key bottom left
set key font "Helvetica,16"
set key box
set key reverse
set key Left

#set title 'Diode - Current at p-contact' font "Helvetica bold,20"
